{
  "message": {
    "containerSelector": "div.message.default",
    "ignoreSelectors": ["div.message.service", "div.message_date", "div.message.joined"]
  },
  "sender": {
    "rules": [
      {
        "selector": "div.from_name",
        "value": { "type": "text" },
        "normalize": { "trim": true, "collapseWhitespace": true }
      }
    ]
  },
  "ts": {
    "rules": [
      {
        "selector": "div.pull_right.date.details[title]",
        "value": { "type": "attr", "name": "title" },
