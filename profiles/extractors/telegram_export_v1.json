      },
      {
        "selector": "div.pull_right.date.details",
        "value": { "type": "text" },
        "normalize": { "trim": true, "collapseWhitespace": true }
      }
    ]
  },
  "text": {
    "rules": [
      {
        "selector": "div.text",
        "value": { "type": "text", "preserveNewlines": true },
        "normalize": { "trim": true }
      }
    ]
